#!/usr/bin/make -f
# Makefile for WineASIO #
# --------------------- #
# Created by falkTX
#

all:
# $(info usage: ${0} 32|64)

# ---------------------------------------------------------------------------------------------------------------------

32:
	$(MAKE) ARCH=i386 M=32

64:
	$(MAKE) ARCH=x86_64 M=64

# ---------------------------------------------------------------------------------------------------------------------

clean::
	rm -f *.o *.so
	rm -rf build32 build64

# ---------------------------------------------------------------------------------------------------------------------

ifneq ($(ARCH),)
ifneq ($(M),)
include Makefile.mk
endif
endif

# ---------------------------------------------------------------------------------------------------------------------
